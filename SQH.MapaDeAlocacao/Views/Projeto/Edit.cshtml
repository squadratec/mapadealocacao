
@{
    ViewData["Title"] = "Edição de Projeto";
}

@model SQH.Entities.Models.Projeto.ProjetoModel

<div class="container">
    <div class="row">
        <div class="col-md-12">

            <h1>Editar Projeto</h1>
            <button type="button" class="btn btn-info" data-toggle="modal" data-target="#modalSalvarTipoAlocacao" data-backdrop="static">Adicionar Tipo de Alocação</button>
            <div class="clearfix"></div>

            <hr />

            @Html.Partial("_gridAlocacao", Model)

            <div class="text-right">
                <button type="button" class="btn btn-default">Voltar</button>
            </div>
        </div>
    </div>
</div>

@Html.Partial("_modalAdicionarTipoAlocacao", Model)

@section Scripts{
    <script src="~/app/js/Projeto/EditProjeto.js"></script>
}
